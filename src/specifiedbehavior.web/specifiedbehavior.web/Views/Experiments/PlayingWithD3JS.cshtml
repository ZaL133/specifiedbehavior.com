@section Scripts {

    <script src="https://d3js.org/d3.v6.min.js"></script>
    <script>
        const data = [
            { color: "Blue", value: 20 },
            { color: "Brown", value: 35 },
            { color: "Green", value: 15 },
            { color: "Hazel", value: 10 },
            { color: "Grey", value: 5 },
            { color: "Other", value: 15 },
        ];

        // Define dimensions
        let width = 600;
        let height = 600;
        let radius = Math.min(width, height) / 2.2;

        // Create SVG element
        let svg = d3.select("#chart")
            .append("svg")
            .attr("width", width)
            .attr("height", height);

        // Create group element to hold pie chart
        let g = svg.append("g")
            .attr("transform", "translate(" + width / 2 + "," + height / 2 + ")");

        // Create color scale
        let color = d3.scaleOrdinal(d3.schemeCategory10);

        // Create pie layout
        let pie = d3.pie().value(function (d) { return d.value; });

        // Create arc generator
        let arc = d3.arc()
            .innerRadius(0)
            .outerRadius(radius);

        // Compute pie layout
        let arcs = pie(data);

        // Draw arcs
        let path = g.selectAll("path")
            .data(arcs)
            .enter()
            .append("path")
            .attr("fill", function (d) { return color(d.data.color); })
            .attr("d", arc)
            .on("mouseover", function (event, d) {
                // Enlarge the radius on mouseover
                let segment = d3.select(this);
                segment.transition()
                    .duration(200)
                    .attr("d", d3.arc().innerRadius(0).outerRadius(radius * 1.1)(d));
            })
            .on("mouseout", function (event, d) {
                // Shrink the radius back to its original size on mouseout
                let segment = d3.select(this);
                segment.transition()
                    .duration(200)
                    .attr("d", arc(d));
            });


        // Add labels
        g.selectAll("text")
            .data(arcs)
            .enter()
            .append("text")
            .attr("transform", function (d) {
                return "translate(" + arc.centroid(d) + ")";
            })
            .attr("text-anchor", "middle")
            .text(function (d) {
                return d.data.color;
            });


    </script>

}

